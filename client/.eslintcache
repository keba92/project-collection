th0-react';\r\nimport React from 'react';\r\nimport { Card } from 'react-bootstrap';\r\nimport {  MarkdownPreview  } from 'react-marked-markdown';\r\nimport { Link } from 'react-router-dom';\r\nimport LikeButton from './LikeButton';\r\nimport Tag from '@uiw/react-tag';\r\nimport { useTranslation } from 'react-i18next';\r\n\r\n\r\nexport default function TabelItems(props) {\r\n    const {dataItems, idCollect} = props;\r\n    const { isAuthenticated } = useAuth0();\r\n    const { t, i18n } = useTranslation();\r\n    const collectItems = [];\r\n\r\n    if(idCollect == ''){\r\n        dataItems.forEach((el) => collectItems.push(el))\r\n    } else {\r\n        dataItems.forEach((el) => {\r\n            if(el.idCollect == idCollect) {\r\n                collectItems.push(el);\r\n            }\r\n        })\r\n    }\r\n    const makeItems = () => {\r\n        if(collectItems.length!=0){\r\n            return collectItems.reverse().map((el, idx) => {\r\n                const data = JSON.parse(el.dataItem);\r\n                const pole = JSON.parse(el.poleItem);\r\n                return (\r\n                    <Card style={{ width: '15rem' }} className='item' key={idx}>\r\n                        <Card.Body>\r\n                            {Object.keys(data).map((keyName, idx) => {\r\n                                // eslint-disable-next-line default-case\r\n                                switch (pole[keyName]) {\r\n                                    case 'number':\r\n                                    return <Card.Text key = {idx}>{`${keyName}: ${data[keyName]}`}</Card.Text>;\r\n                                    case 'text':\r\n                                    return <Card.Text key = {idx}>{`${keyName}: ${data[keyName]}`}</Card.Text>;\r\n                                    case 'textarea':\r\n                                    return <MarkdownPreview value={data[keyName]} />;\r\n                                    case 'date':\r\n                                    return <Card.Text key = {idx}>{`${keyName}: ${data[keyName]}`}</Card.Text>;\r\n                                    case 'checkbox':\r\n                                    return <Card.Text key = {idx}>{`${data['name']}: ${(data[keyName])?('Да'):('Нет')}`}</Card.Text>;\r\n                                }\r\n                            })}\r\n                            {el.tag.map((teg,indx)=>{\r\n                                return (\r\n                                    <Tag key={`${indx}`} light disabled color=\"#28a745\">{teg}</Tag>\r\n                                )\r\n                            })}\r\n                        </Card.Body>\r\n                        {(isAuthenticated)&&(<LikeButton id={el._id}/>)}\r\n                        <Link to={`/item/${el._id}`}> {t('openL')} </Link>\r\n                    </Card>\r\n                )\r\n            })\r\n        }\r\n    }\r\n    \r\n\r\n    return (\r\n        <div className='table-items'>\r\n            {makeItems()}\r\n        </div>\r\n    )\r\n}","D:\\Itransition\\project\\client\\src\\components\\Home.js",["163","164","165","166"],"D:\\Itransition\\project\\client\\src\\components\\privateRoute.js",[],"D:\\Itransition\\project\\client\\src\\components\\UserProfileButton.js",["167"],"import React from 'react';\r\nimport { useAuth0 } from '@auth0/auth0-react';\r\nimport { Link } from 'react-router-dom';\r\nimport { useTranslation } from 'react-i18next';\r\n\r\nconst UserProfileButton = () => {\r\n  const { user, isAuthenticated } = useAuth0();\r\n  const { t, i18n } = useTranslation();\r\n\r\n  return (\r\n    isAuthenticated && (\r\n      <Link className='back' to={`/user/${user.sub}`}>\r\n        {t('userPageL')}\r\n      </Link>\r\n    )\r\n  )\r\n}\r\n\r\nexport default UserProfileButton","D:\\Itransition\\project\\client\\src\\components\\ItemInfo.js",["168","169","170","171","172","173","174"],"D:\\Itransition\\project\\client\\src\\components\\TableCollection.js",["175","176","177",